# CodePush Server - QA Environment (Private EC2 with IAM Role)
# No AWS keys needed - EC2 instance uses IAM role for S3 access

# Environment Configuration
NODE_ENV=development
BASE_URL=
LOGGING=true
ENABLE_PACKAGE_DIFFING=true
ENABLE_ACCOUNT_REGISTRATION=true

# AWS S3 Configuration (IAM role provides access automatically)
AWS_BUCKET_NAME=crabi-qa-code-push-server
AWS_REGION=us-east-1

# Debug Settings (enabled for QA testing - bypasses OAuth)
DEBUG_DISABLE_AUTH=true
DEBUG_USER_ID=qa-test-user

# AWS Redis Configuration (existing QA Redis cluster)
REDIS_HOST=cache.9eczd4.0001.use1.cache.amazonaws.com
REDIS_PORT=6379
REDIS_DB=0
REDIS_TLS_DISABLED=true
# GitHub OAuth (not needed in QA due to debug mode)
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=

# Server Configuration
PORT=3000
CODE_PUSH_SERVER_URL=http://codepush.qa.crabi.net:3000
