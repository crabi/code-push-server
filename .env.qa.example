# CodePush Server - QA Environment (Public HTTPS with ALB)
# Updated for HTTPS deployment with proper authentication

# Environment Configuration
NODE_ENV=development
BASE_URL=
LOGGING=true
ENABLE_PACKAGE_DIFFING=true
ENABLE_ACCOUNT_REGISTRATION=true

# AWS S3 Configuration (IAM role provides access automatically)
AWS_BUCKET_NAME=crabi-qa-code-push-server
AWS_REGION=us-east-1

# Authentication Settings - OAuth enabled for production-like testing
DEBUG_DISABLE_AUTH=false
DEBUG_USER_ID=qa-test-user

# AWS Redis Configuration (existing QA Redis cluster)
REDIS_HOST=cache.9eczd4.0001.use1.cache.amazonaws.com
REDIS_PORT=6379
REDIS_DB=0
REDIS_TLS_DISABLED=true

# GitHub OAuth (REQUIRED - add your credentials)
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret

# Server Configuration
PORT=3000
CODE_PUSH_SERVER_URL=https://codepush.qa.crabi.net
